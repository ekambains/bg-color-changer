{
    "manifest_version": 3,
    "version": "1.0.0",
    "name": "Background Color Changer",
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "scripting",
        "activeTab"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "icons": {
        "32": "icon32.png"
    }
}